<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bettercliq">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BetterCLiQ">
        <activity
            android:name=".ProfileScreen"
            android:exported="false" />
        <activity
            android:name=".formActicvity.BugReport"
            android:exported="false" />
        <activity
            android:name=".formActicvity.PraiseFeature"
            android:exported="false" />
        <activity
            android:name=".formActicvity.FeatureSuggestion"
            android:exported="false" />
        <activity
            android:name=".formActicvity.FeatureEnhancment"
            android:exported="false" />
        <activity
            android:name=".HomeScreeen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".SignUpScreen"
            android:exported="false">

        </activity>
        <activity
            android:name=".SplashScreen"
            android:exported="false"
            android:theme="@style/Theme.BetterCLiQSplashScreen" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>